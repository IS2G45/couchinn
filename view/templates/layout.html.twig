<!doctype html>
<html lang="es">
    <head>
        <meta charset="UTF-8">
        <title>Couchinn</title>
        <meta name="description" content="Sistema de publicaci&oacute;n de hospedajes">
        <meta name="author" content="Grupo 45 - Ingenieria de sotware 2 - 2016">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% block stylesheet %}
            <link rel="stylesheet" href="{{ constant('PATH_URL_CSS') }}layout.css" type="text/css" /> 
            <link rel="stylesheet" href="{{ constant('PATH_URL_VENDOR') }}jquery/jquery-ui-1.11.4/jquery-ui.css" type="text/css" />
            <link rel="stylesheet" href="{{ constant('PATH_URL_VENDOR') }}twbs/bootstrap/dist/css/bootstrap.min.css" type="text/css" />
        {% endblock %}
        {% block otherStylesheet  %}
        {% endblock %}
    </head>
    <body>
        {% block javascripts %}
            <script src="{{ constant('PATH_URL_VENDOR') }}jquery/jquery-2.2.3.min.js"></script>
            <script src="{{ constant('PATH_URL_VENDOR') }}jquery/jquery-ui-1.11.4/jquery-ui.min.js"></script>
            <script src="{{ constant('PATH_URL_VENDOR') }}twbs/bootstrap/js/dropdown.js"></script>
        {% endblock %}
        {% block otherJavascripts %}
        {% endblock %}
        {% block  header %}
            <div id="header" class="row">
                <div class="col-md-4 col-md-offset-2 contenedorLogo">
                    <img src="{{ constant('PATH_URL_IMG') }}couchinnLogo.png" style="height: 60px;" alt="Logo de Couchinn">
                </div>
                <div class="col-md-4 text-center">
                    <div class="col-md-3 botones">
                        <a href="#">NOSOTROS</a>
                    </div>
                    <div class="col-md-3 botones">
                        <a href="#">¿COUCHINN?</a>
                    </div>
                    <div class="col-md-3 botones">
                        <a href="#">COUCH'S</a>
                    </div>
                    <div class="col-md-3 botones">
                        <a href="#">INGRESAR</a>
                    </div>
                </div>
            </div>
        {% endblock %}
        {% block container %}
        {% endblock %}
        {% block footer %}
            <div id="footer" class="row" >
                <div class="col-md-3">
                    <div class="col-md-6 logoFooter text-center">
                        <img src="{{ constant('PATH_URL_IMG') }}couchinnLogo.png" style="height: 45px;" alt="Logo de Couchinn">
                    </div>
                    <div class="col-md-6 text-center contenedorAddres">
                        <address>
                            <strong>Oficina Central</strong><br>
                            Calle 12 nro 1250<br>
                            Piso 16, Oficina 20<br>
                            La Plata, Buenos Aires<br>
                            <abbr title="Teléfono">Tel&eacute;fono:</abbr> (0221) 424-7890
                        </address>
                        <address>
                            <strong>Contacto</strong><br>
                            <a href="mailto:#">informacion@couchinn.com</a>
                        </address>
                    </div>    
                </div>
                <div id="mapa" class="col-md-9 text-right">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d13085.02842011937!2d-57.95366665!3d-34.92508835!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0000000000000000%3A0x9322890c84a35ff6!2sTorre+Gubernamental+I!5e0!3m2!1ses-419!2sar!4v1463171422349" width="100%" height="150px" frameborder="0" style="border:0" allowfullscreen></iframe>
                </div>
            </div>
        {% endblock %}
    </body>
</html>